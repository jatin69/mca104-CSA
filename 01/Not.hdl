/**
 * Not gate:
 * out = not in
 */

/**
 * Author : Jatin Rohilla
 * Course : MCA 104
 * 
 * Dependencies : Nand
 */


CHIP Not {
    IN in;
    OUT out;

    PARTS:
    
    /* Classic Implementation of a Not gate using Nand gates */

    Nand(a=in,b=in,out=out);
}